<h1>Edit Employee</h1>
<%= form_for @employee do |f|%>
    Dog: <%= f.collection_select :dog_id, Dog.all, :id, :name %>
    <br />
    <%=f.label :first_name%>
    <%= f.text_field :first_name%>
    <br />
    <%=f.label :last_name%>
    <%= f.text_field :last_name%>
    <br />
    <%=f.label :alias%>
    <%= f.text_field :alias%>
    <br />
    <%=f.label :title%>
    <%= f.text_field :title%>
    <br />
    <%=f.label :office%>
    <%= f.text_field :office%>
    <br />
    <%=f.label :image_URL%>
    <%= f.text_field :img_url%>
    <br />
    <%=f.submit%>
    <% if flash[:notice] %>
    <p><div class="notice"><%= flash[:notice] %></div></p>
  <% end %>
<%end%>
