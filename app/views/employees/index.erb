<h1>All Employees</h1>

<% @employees.each do |employee| %> 
    <%=link_to employee.first_name %>
    <%=link_to employee.last_name %>
    <%=link_to employee.alias %>
    <%=link_to employee.title %>
    <%=link_to employee.office %>
    <%=link_to employee.img_url %>
    <%=link_to 'Edit', edit_employee_path(employee)%>
<% end %>