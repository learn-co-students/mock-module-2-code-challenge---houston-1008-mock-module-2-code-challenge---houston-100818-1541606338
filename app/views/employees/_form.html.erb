<%= form_for @employee do |form| %>
    <%= form.label "Name"%>
    <br>
    <%= form.label "First: "%>
    <%= form.text_field :first_name %>
    <%= form.label "Last: "%>
    <%= form.text_field :last_name %>
    <br>
    <%= form.label "Title: "%>
    <%= form.text_field :title %>
    <%= form.label "Office location: " %>
    <%= form.text_field :office %>
    <br>
    <%= form.label "Alias: "%>
    <%= form.text_field :alias %>
    <br>
    <%= form.label "Image URL"%>
    <%= form.text_field :img_url, value: @img %>
    <br>
    <%= form.collection_select :dog_id, @dogs, :id, :name %>
    <br>
    <%= form.submit %>
    
<% end %>